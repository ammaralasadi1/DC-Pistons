<h2>New Article</h2>

<%= form_for @article do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :body %>
  <%= f.text_field :body %>

  <%= f.label :photo_url %>
  <%= f.text_field :photo_url %>

  <%= f.label :author_name %>
  <%= f.text_field :author_name %>

  <%= f.submit %>
<% end %>
